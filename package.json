{
  "name": "aniarchive-gql",
  "description": "GraphQL API for Animation Archive",
  "author": {
    "email": "drew@pow.rs",
    "name": "Drew Powers"
  },
  "license": "MIT",
  "version": "0.0.2",
  "repository": {
    "type": "github",
    "url": "https://github.com/drwpow/aniarchive-gql"
  },
  "scripts": {
    "build": "npm run generate && tsc app.ts",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly app",
    "generate": "prisma generate",
    "lint": "eslint --ext .ts 'src/**/*'",
    "migrate": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "seed": "node ./prisma/seed.js",
    "start": "node ./app.js"
  },
  "dependencies": {
    "@nexus/schema": "^0.18.0",
    "@prisma/cli": "^2.11.0",
    "@prisma/client": "^2.11.0",
    "@prisma/sdk": "^2.11.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0",
    "graphql-yoga": "^1.18.3",
    "nexus-plugin-prisma": "^0.24.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.0"
  }
}
